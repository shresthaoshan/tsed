		"@tsed/json-mapper": "^6.62.2",
		"@tsed/mongoose": "^6.62.2",
		"@tsed/platform-express": "^6.62.2",
		"@tsed/schema": "^6.62.2",
		"@tsed/swagger": "^6.62.2",
		"@types/jsonwebtoken": "^8.5.4",
		"ajv": "^8.6.2",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
