		"@types/method-override": "^0.0.32",
		"@types/multer": "^1.4.7",
		"@types/node": "^16.6.0",
		"ts-node": "^10.2.0",
		"ts-node-dev": "^1.1.8",
		"tsconfig-paths": "^3.10.1",
